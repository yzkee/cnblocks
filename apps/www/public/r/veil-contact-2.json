      "content": "import { Button } from '@/components/ui/button'\nimport { Card } from '@/components/ui/card'\nimport { Input } from '@/components/ui/input'\nimport { Label } from '@/components/ui/label'\nimport { Textarea } from '@/components/ui/textarea'\nimport { Select, SelectContent, SelectItem, SelectTrigger, SelectValue } from '@/components/ui/select'\n\nexport default function Contact() {\n    return (\n        <section className=\"bg-background @container py-24\">\n            <div className=\"mx-auto max-w-2xl px-6\">\n                <div className=\"text-center\">\n                    <h1 className=\"text-balance font-serif text-4xl font-medium sm:text-5xl\">Contact Sales</h1>\n                    <p className=\"text-muted-foreground mx-auto mt-4 max-w-md text-balance\">Ready to get started? Our team will help you find the right plan for your business.</p>\n                </div>\n\n                <Card\n                    variant=\"outline\"\n                    className=\"mt-12 p-8\">\n                    <form\n                        action=\"\"\n                        className=\"space-y-5\">\n                        <div className=\"@md:grid-cols-2 grid gap-4\">\n                            <div className=\"space-y-2\">\n                                <Label\n                                    htmlFor=\"firstName\"\n                                    className=\"text-sm\">\n                                    First name\n                                </Label>\n                                <Input\n                                    type=\"text\"\n                                    id=\"firstName\"\n                                    name=\"firstName\"\n                                    required\n                                />\n                            </div>\n                            <div className=\"space-y-2\">\n                                <Label\n                                    htmlFor=\"lastName\"\n                                    className=\"text-sm\">\n                                    Last name\n                                </Label>\n                                <Input\n                                    type=\"text\"\n                                    id=\"lastName\"\n                                    name=\"lastName\"\n                                    required\n                                />\n                            </div>\n                        </div>\n\n                        <div className=\"space-y-2\">\n                            <Label\n                                htmlFor=\"email\"\n                                className=\"text-sm\">\n                                Work email\n                            </Label>\n                            <Input\n                                type=\"email\"\n                                id=\"email\"\n                                name=\"email\"\n                                required\n                            />\n                        </div>\n\n                        <div className=\"space-y-2\">\n                            <Label\n                                htmlFor=\"company\"\n                                className=\"text-sm\">\n                                Company\n                            </Label>\n                            <Input\n                                type=\"text\"\n                                id=\"company\"\n                                name=\"company\"\n                            />\n                        </div>\n\n                        <div className=\"@md:grid-cols-2 grid gap-4\">\n                            <div className=\"space-y-2\">\n                                <Label\n                                    htmlFor=\"size\"\n                                    className=\"text-sm\">\n                                    Company size\n                                </Label>\n                                <Select>\n                                    <SelectTrigger className=\"ring-input focus-visible:ring-ring/15 bg-card h-8 shadow-none dark:bg-transparent\">\n                                        <SelectValue placeholder=\"Select\" />\n                                    </SelectTrigger>\n                                    <SelectContent className=\"ring-border rounded-xl border-transparent ring-1\">\n                                        <SelectItem value=\"1-10\">1-10 employees</SelectItem>\n                                        <SelectItem value=\"11-50\">11-50 employees</SelectItem>\n                                        <SelectItem value=\"51-200\">51-200 employees</SelectItem>\n                                        <SelectItem value=\"201-500\">201-500 employees</SelectItem>\n                                        <SelectItem value=\"500+\">500+ employees</SelectItem>\n                                    </SelectContent>\n                                </Select>\n                            </div>\n                            <div className=\"space-y-2\">\n                                <Label\n                                    htmlFor=\"interest\"\n                                    className=\"text-sm\">\n                                    Interest\n                                </Label>\n                                <Select>\n                                    <SelectTrigger className=\"ring-input focus-visible:ring-ring/15 bg-card h-8 shadow-none dark:bg-transparent\">\n                                        <SelectValue placeholder=\"Select\" />\n                                    </SelectTrigger>\n                                    <SelectContent className=\"ring-border rounded-xl border-transparent ring-1\">\n                                        <SelectItem value=\"pricing\">Pricing</SelectItem>\n                                        <SelectItem value=\"demo\">Product demo</SelectItem>\n                                        <SelectItem value=\"partnership\">Partnership</SelectItem>\n                                        <SelectItem value=\"other\">Other</SelectItem>\n                                    </SelectContent>\n                                </Select>\n                            </div>\n                        </div>\n\n                        <div className=\"space-y-2\">\n                            <Label\n                                htmlFor=\"message\"\n                                className=\"text-sm\">\n                                Message\n                            </Label>\n                            <Textarea\n                                id=\"message\"\n                                name=\"message\"\n                                rows={5}\n                                placeholder=\"Tell us about your needs...\"\n                                className=\"min-h-28\"\n                            />\n                        </div>\n\n                        <Button className=\"w-full\">Submit</Button>\n                    </form>\n                </Card>\n\n                <p className=\"text-muted-foreground mt-6 text-center text-sm\">\n                    By submitting, you agree to our{' '}\n                    <a\n                        href=\"#\"\n                        className=\"text-foreground underline\">\n                        Privacy Policy\n                    </a>\n                </p>\n            </div>\n        </section>\n    )\n}\n",
