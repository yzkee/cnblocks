{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "veil-contact-1",
  "type": "registry:block",
  "title": "Contact 1",
  "description": "Tailark Veil contact block",
  "registryDependencies": [
    "veil-button",
    "veil-card",
    "veil-input",
    "veil-textarea",
    "@shadcn/label"
  ],
  "files": [
    {
      "path": "../../packages/veil-kit/blocks/contact/one/index.tsx",
      "content": "import { Button } from '@/components/ui/button'\nimport { Card } from '@/components/ui/card'\nimport { Input } from '@/components/ui/input'\nimport { Label } from '@/components/ui/label'\nimport { Textarea } from '@/components/ui/textarea'\nimport Link from 'next/link'\nimport { Mail, MapPin, Phone } from 'lucide-react'\n\nexport default function Contact() {\n    return (\n        <section className=\"bg-background @container py-24\">\n            <div className=\"mx-auto max-w-3xl px-6\">\n                <div>\n                    <h1 className=\"text-balance font-serif text-4xl font-medium sm:text-5xl\">Get in Touch</h1>\n                    <p className=\"text-muted-foreground mt-4 max-w-md text-balance\">Have questions? We'd love to hear from you. Send us a message and we'll respond as soon as possible.</p>\n                </div>\n\n                <div className=\"@xl:grid-cols-5 mt-12 grid gap-8\">\n                    <div className=\"@xl:col-span-2 space-y-6 *:space-y-2\">\n                        <div>\n                            <p className=\"text-foreground text-sm font-medium\">Email</p>\n                            <Link\n                                href=\"mailto:hello@example.com\"\n                                className=\"text-muted-foreground hover:text-primary text-sm\">\n                                hello@example.com\n                            </Link>\n                        </div>\n\n                        <div>\n                            <p className=\"text-foreground text-sm font-medium\">Phone</p>\n                            <Link\n                                href=\"tel:+1234567890\"\n                                className=\"text-muted-foreground hover:text-primary text-sm\">\n                                +1 (234) 567-890\n                            </Link>\n                        </div>\n\n                        <div>\n                            <p className=\"text-foreground text-sm font-medium\">Office</p>\n                            <p className=\"text-muted-foreground text-sm\">123 Main Street, San Francisco, CA 94102</p>\n                        </div>\n                    </div>\n\n                    <Card\n                        variant=\"outline\"\n                        className=\"@xl:col-span-3 p-6\">\n                        <form\n                            action=\"\"\n                            className=\"space-y-5\">\n                            <div className=\"@md:grid-cols-2 grid gap-4\">\n                                <div className=\"space-y-2\">\n                                    <Label\n                                        htmlFor=\"name\"\n                                        className=\"text-sm\">\n                                        Name\n                                    </Label>\n                                    <Input\n                                        type=\"text\"\n                                        id=\"name\"\n                                        name=\"name\"\n                                        placeholder=\"Your name\"\n                                        required\n                                    />\n                                </div>\n                                <div className=\"space-y-2\">\n                                    <Label\n                                        htmlFor=\"email\"\n                                        className=\"text-sm\">\n                                        Email\n                                    </Label>\n                                    <Input\n                                        type=\"email\"\n                                        id=\"email\"\n                                        name=\"email\"\n                                        placeholder=\"you@example.com\"\n                                        required\n                                    />\n                                </div>\n                            </div>\n\n                            <div className=\"space-y-2\">\n                                <Label\n                                    htmlFor=\"subject\"\n                                    className=\"text-sm\">\n                                    Subject\n                                </Label>\n                                <Input\n                                    type=\"text\"\n                                    id=\"subject\"\n                                    name=\"subject\"\n                                    placeholder=\"How can we help?\"\n                                />\n                            </div>\n\n                            <div className=\"space-y-2\">\n                                <Label\n                                    htmlFor=\"message\"\n                                    className=\"text-sm\">\n                                    Message\n                                </Label>\n                                <Textarea\n                                    id=\"message\"\n                                    name=\"message\"\n                                    rows={4}\n                                    placeholder=\"Tell us more...\"\n                                    required\n                                    className=\"min-h-28\"\n                                />\n                            </div>\n\n                            <Button className=\"w-full\">Send Message</Button>\n                        </form>\n                    </Card>\n                </div>\n            </div>\n        </section>\n    )\n}\n",
      "type": "registry:component",
      "target": "components/contact-1.tsx"
    }
  ]
}